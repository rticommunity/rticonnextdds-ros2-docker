<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.1/rti_dds_qos_profiles.xsd"
     version="6.0.1">
  <qos_library name="connext_docker_examples">
    <qos_profile name="Default" is_default_qos="true">
      <participant_qos>
        <!-- Use only the builtin shared-memory transport -->
        <transport_builtin>
          <mask>SHMEM</mask>
        </transport_builtin>
      </participant_qos>
    </qos_profile>
  </qos_library>
</dds>
